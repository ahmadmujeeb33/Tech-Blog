

{{#each allData as |Data|}}

    <a href="/BlogInfo/{{Data.id}}"><div class="Blog-Posts">
        <h1>Ttile</h1>

        <p>{{Data.title}}</p>

        <h1>Content</h1>
        <p>{{Data.content}}</p>

    </div></a>

    <h1>User</h1>
    <p>{{Data.User.username}}</p>

    <h1>Comments</h1>

    {{#each Data.Comments as |comments| }}
        <p>{{comments.content}}</p>
    {{/each}}
       

{{/each}}